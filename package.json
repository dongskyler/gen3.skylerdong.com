{
  "name": "gen3.skylerdong.com",
  "version": "0.1.0",
  "description": "Working repository for my 3rd-generation personal website at skylerdong.com",
  "repository": "https://github.com/dongskyler/gen3.skylerdong.com.git",
  "author": "Skyler Dong <skyler@skylerdong.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ci": "yarn --frozen-lockfile",
    "format": "prettier './**/*.{js,jsx,ts,tsx,json,html}' --write",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --cache --fix",
    "clean": "rimraf build",
    "build": "run-s clean && react-scripts build",
    "test": "react-scripts test",
    "start": "NODE_ENV=development && run-s ci && react-scripts start",
    "serve": "serve -s build",
    "production": "NODE_ENV=production run-s ci build serve",
    "eject": "react-scripts eject",
    "refresh": "ramraf node_modules && run-s ci"
  },
  "engines": {
    "node": ">=0.12 <0.14"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,html}": [
      "prettier --write"
    ],
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.3.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.0.0",
    "react-router": "^4.3.0",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/dom": "^7.19.0",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "~2.34.0",
    "@typescript-eslint/parser": "~2.34.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "eslint": "~6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "~2.5.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "serve": "^11.3.2",
    "typescript": "^3.9.5"
  }
}
